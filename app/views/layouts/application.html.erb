<!DOCTYPE html>
<html>
  <head>
    <title>Controle Pedido</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body class="tudo">
    <header>
      <% if usuario_signed_in? %>
        <%= render 'layouts/nav_bar_user' %>
        <div class="container">
          <div class="alert alert-info" role="alert">
            <%= breadcrumbs pretext: "",
                separator: " &rsaquo; " %>
          </div>
        </div>
      <% end %>
    </header>
    <div class="container" >
      <%= yield %>
      <div class="text-center m-t-lg">
        <strong>Copyright</strong> Jean Andreolla &copy; <%= Date.today.year %>
      </div>
    </div>
  </body>
</html>
