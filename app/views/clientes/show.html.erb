<div class="container">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Nome:</strong>
    <%= @cliente.nome %>
  </p>

  <p>
    <strong>Cpf:</strong>
    <%= @cliente.cpf %>
  </p>

  <p>
    <strong>Endereco:</strong>
    <%= @cliente.endereco %>
  </p>

  <p>
    <strong>Cidade:</strong>
    <%= @cliente.cidade %>
  </p>

  <p>
    <strong>Uf:</strong>
    <%= @cliente.uf %>
  </p>

  <p>
    <strong>Fone:</strong>
    <%= @cliente.fone %>
  </p>

  <div class="btn-group" >
      <%= link_to 'Editar', edit_cliente_path(@cliente), :class => "btn btn-primary" %>
      <%= link_to 'Voltar', clientes_path, :class => "btn btn-primary" %>
  </div>
</div>
